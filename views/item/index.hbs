<h3>Inventory Item List</h3>

 <table class="table table-striped">
     <thead>
         <tr>
             <th>Name</th>
             <th>Category</th>
             <th>Quantity</th>
             <th>Price</th>
         </tr>
     </thead>
     <tbody>
         {{#each list}}
         <tr>
            <td>{{this.name}}</td>
            <td>{{this.category}}</td>
            <td>{{this.quantity}}</td>
            <td>{{this.price}}</td>
            <td>
                <a href="/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
            </td>
         </tr>
         {{/each}}
     </tbody>
 </table>